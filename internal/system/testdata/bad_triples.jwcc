[
  // More than 4 components should fail.
  "foo-bar-baz-quux-xyzzy",
  // If something comes after cygwin or mingw
  // (which both imply an environment),
  // then error out.
  "x86_64-pc-cygwin-cygnus",
  "x86_64-cygwin-cygnus",
  "x86_64-pc-mingw-gnu",
  "x86_64-mingw-gnu",
  // Single components should fail.
  "foo",
  "linux",
  "darwin",
]
